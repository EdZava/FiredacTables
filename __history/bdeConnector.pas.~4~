unit bdeConnector;

interface

uses Classes, SysUtils, {$ifdef FIREDAC} fireTables {$else} dbTables {$endif};


type
   TQuery = {$ifdef FIREDAC} TFireQuery {$else} dbTables.TQuery {$endif};
   TTable = {$ifdef FIREDAC} TFireTable {$else} dbTables.TTable {$endif};
   TStoredProc = {$ifdef FIREDAC} TFireStoredProc {$else} dbTables.TStoredProc {$endif};
   TSession = {$ifdef FIREDAC} TFireSession {$else} dbTables.TSession {$endif};
   TDatabase = {$ifdef FIREDAC} TFireDatabase {$else} dbTables.TDatabase {$endif};



implementation

end.
